{{>header}}

import type { BadGateway as ClientGenericBadGateway } from './models/BadGateway';
import type { Forbidden as ClientGenericForbidden } from './models/Forbidden';
import type { InternalServerError as ClientGenericInternalServerError } from './models/InternalServerError';
import type { NotFound as ClientGenericNotFound } from './models/NotFound';
import type { ServiceUnavailable as ClientGenericServiceUnavailable } from './models/ServiceUnavailable';
import type { Unauthorized as ClientGenericUnauthorized } from './models/Unauthorized';
import type { NetworkUnknownError as ClientGenericNetworkUnknownError } from './models/NetworkUnknownError';
import type { NetworkError as ClientGenericNetworkError } from './models/NetworkError';
import type { BadRequest as ClientGenericBadRequest } from './models/BadRequest';

{{#if @root.exportClient}}
export { {{{clientName}}} } from './{{{clientName}}}';

{{/if}}
{{#if @root.exportCore}}
export { ApiError } from './core/ApiError';
{{#if @root.exportClient}}
export { BaseHttpRequest } from './core/BaseHttpRequest';
{{/if}}
export { CancelablePromise, CancelError } from './core/CancelablePromise';
export { OpenAPI } from './core/OpenAPI';
export type { OpenAPIConfig } from './core/OpenAPI';
{{/if}}
{{#if @root.exportModels}}
{{#if models}}

{{#each models}}
{{#if @root.useUnionTypes}}
export type { {{{name}}} } from './models/{{{name}}}';
{{else if enum}}
export { {{{name}}} } from './models/{{{name}}}';
{{else if enums}}
export { {{{name}}} } from './models/{{{name}}}';
{{else}}
export type { {{{name}}} } from './models/{{{name}}}';
{{/if}}
{{/each}}
{{/if}}
{{/if}}
{{#if @root.exportSchemas}}
{{#if models}}

export type ClientGenericErrors =
  | ClientGenericBadGateway
  | ClientGenericForbidden
  | ClientGenericInternalServerError
  | ClientGenericNotFound
  | ClientGenericServiceUnavailable
  | ClientGenericUnauthorized
  | ClientGenericNetworkUnknownError
  | ClientGenericNetworkError
  | ClientGenericBadRequest

{{#each models}}
export { ${{{name}}} } from './schemas/${{{name}}}';
{{/each}}
{{/if}}
{{/if}}
{{#if @root.exportServices}}
{{#if services}}

{{#each services}}
export { {{{name}}}{{{@root.postfix}}} } from './services/{{{name}}}{{{@root.postfix}}}';
{{/each}}
{{/if}}
{{/if}}
